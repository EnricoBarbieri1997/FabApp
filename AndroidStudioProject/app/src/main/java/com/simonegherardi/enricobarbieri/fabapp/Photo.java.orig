    @Override
    public int compareTo(@NonNull Photo o) {
        if (this.idPhoto == o.idPhoto)
        {
            return 0;
        } else if (this.idPhoto < o.idPhoto)
        {
            return -1;
        } else
        {
            return 1;
        }
    }
}
