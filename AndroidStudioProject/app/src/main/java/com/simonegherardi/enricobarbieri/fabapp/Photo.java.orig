public class Photo extends Resource{

    protected User photographed;
    protected SingleUser photographer;
    protected String url;
    protected Bitmap img;
    protected int likes;
    protected int iDPhotographer;
    protected int iDPhotograpghed;
    public static Photo FromJSON(JSON json)
    {
        Photo photo = new Photo;
        try {
            photo.Init(json.GetInt("id"), null, null, json.GetString("url"));
        } catch (JSONParseException e) {
            e.printStackTrace();
        }
        return photo;
    }
    public Photo()
    {
        super();
    }
    public void Init(Integer id, User photographed, SingleUser photographer, String url)
    {
        this.id = id;
        this.photographed = photographed;
        this.photographer = photographer;
        this.url = url;
    }
